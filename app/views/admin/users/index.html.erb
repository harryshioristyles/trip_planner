<h1>Admin::Users#index</h1>
<p>Find me in app/views/admin/users/index.html.erb</p>
	<% @users.each do |user| %>
		<p>UserID:<%= user.id %></p>
		<p>Nickname：<%= user.nickname %></p>
		<p>Mail：<%= user.email %></p>
		<p>Introduce：<%= user.introduction %></p>

		<%= user.user_image.attached? ? image_tag(user.user_image) : image_tag("noimage/no_image.jpg") %>

		<%= link_to "edit", edit_admin_user_path(user.id) %>
	<% end %>